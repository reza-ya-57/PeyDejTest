@model Tuple<string, Dictionary<string,string?>>
<div class="row mb-2 mb-xl-3">
    <div class="col-auto d-none d-sm-block">
        <h3><strong>@Model.Item1</strong></h3>
    </div>
    <div class="col-auto ms-auto text-end mt-n1">
        @foreach (KeyValuePair<string, string?> item in Model.Item2)
        {
            <a href="@item.Value" class="btn btn-light bg-white me-2">@item.Key</a>
        }
    </div>
</div>